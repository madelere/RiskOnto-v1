        <rdfs:subClassOf rdf:resource="https://cs.unb.ca/ontologies/riskonto#Threat"/>
        <rdfs:comment>This class is intended to group all Threats with severityLevel High and no mitigation (i.e., no isMitigatedBy relation). Should be inferred via SWRL.</rdfs:comment>
    </owl:Class>
    


