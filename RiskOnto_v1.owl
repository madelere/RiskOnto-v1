        <owl:versionIRI rdf:resource="https://cs.unb.ca/ontologies/riskonto/1.0"/>
        <ns6:creator>Michael Adekunle Adelere
Luigi Benedicenti
Yasser Morgan</ns6:creator>
        <ns6:date>2025-07-29</ns6:date>
        <ns6:description>An integrated cybersecurity ontology that connects NIST CSF subcategories to NIST SP 800-53 controls and MITRE D3FEND techniques. It includes tooling compliance, threat modeling, SWRL-driven inference, explainable AI recommendations, and quantified risk reasoning.</ns6:description>
        <ns6:publisher>University of New Brunswick</ns6:publisher>
        <ns6:title>RiskOnto for Risk Ontology</ns6:title>
        <rdfs:comment>This ontology provides an end-to-end semantic framework for automated compliance monitoring, mitigation recommendation, and risk scoring in cybersecurity.</rdfs:comment>
        <rdfs:label>RiskOnto</rdfs:label>
        <owl:versionInfo>v1.0</owl:versionInfo>
    </owl:Ontology>
    


